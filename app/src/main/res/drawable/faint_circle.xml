<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <shape android:shape="rectangle">


        <gradient
            android:startColor="@color/colorOrangePrimary"
            android:endColor="@color/colorOrangeDark"
            android:gradientRadius="120dp"
            android:angle="90"/>
        <corners
            android:radius="80dp"
            />
        <size
            android:width="100dp"
            android:height="100dp"/>

        <stroke android:width="12dp"
            android:color="#ffa726"
            />

    </shape>
</item>
</selector>